"http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.fhibernate.Student" table="student">
		<id name="sid" column="sid"/>
		<property name="name" column="name"/>
		<property name="address" column="address"/>

   </class>
</hibernate-mapping>