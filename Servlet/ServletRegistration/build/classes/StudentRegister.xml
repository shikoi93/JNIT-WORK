<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>


<class name="com.servlethibernate.StudentRegister" table="student">
<id name="sid" column="sid"/>
<property name="fullName" column="fullName" length="50"/>
<property name="passwprd" column="password" length="50"/>
<property name="email" column="email" length="50"/>
<property name="phone" column="phone" length="50"/>
<property name="major" column="major" length="50"/>



</class>
</hibernate-mapping>