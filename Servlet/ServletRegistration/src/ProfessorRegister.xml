<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        

<hibernate-mapping>
<class name="com.servlethibernate.RegisterProfessor" table="professor">
<id name="pid" column="pid"/>
<property name="fullName" column="fullName" length="50"/>
<property name="password" column="password" length="50"/>
<property name="email" column="email" length="50"/>
<property name="phone" column="phone" length="50"/>
<property name="department" column="department" length="50"/>



</class>
</hibernate-mapping>